{
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "rewrites": [
      { "source": "/service-worker.js", "destination": "/service-worker.js" }
    ],
    "headers": [
      { "source": "/images/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000" }] }
    ],
    "cleanUrls": true
  }
  